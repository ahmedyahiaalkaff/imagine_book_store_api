			"item": [
				{
					"name": "Login_ValidData",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"const baseURL = pm.variables.get(\"baseURL\");",
									"const newEmail = pm.variables.replaceIn('{{$randomEmail}}');",
									"",
									"pm.globals.set(\"newEmail\", newEmail);",
									"const signupDTO = {",
									"    name: \"John Doe\",",
									"    email: `${newEmail}`,",
									"    password: \"mylongpassword\"",
									"}",
									"console.log(signupDTO);",
									"console.log(JSON.stringify(signupDTO));",
									"",
									"pm.sendRequest({",
									"    url: `${baseURL}/auth/signup`,",
									"    method: 'POST',",
									"    header: {",
									"        'Content-Type': 'application/json',",
									"    },",
									"    body: {",
									"        mode: \"raw\",",
									"        raw: JSON.stringify(signupDTO)",
									"    },",
									"}, function (err, res) {",
									"    if (err || res.err) {",
									"        console.log(\"error \", err);",
									"    } else {",
									"        console.log(res.status);",
									"    }",
									"});",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"email\": \"{{newEmail}}\",\n    \"password\": \"mylongpassword\"\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{baseURL}}/auth/login",
							"host": [
								"{{baseURL}}"
							],
							"path": [
								"auth",
								"login"
							]
						}
					},
					"response": []
				},
